	"keywords" : [
		"bitcoin",
		"cryptocurrency",
		"billing",
		"ecommerce",
		"payment"
	],
	"type" : "library",
	"license" : "MIT",
	"support" : {
		"issues" : "https://github.com/Ekliptor/cashp/issues",
		"source" : "https://github.com/Ekliptor/cashp"
	},
	"authors" : [{
			"name" : "Ekliptor",
			"email" : "Ekliptor1@yandex.com"
		}
	],
	"require": {
		"ekliptor/qr-code": "^1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^8"
	},
	"autoload" : {
